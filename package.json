{
  "name": "11ty-netlify-starter",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "https://github.com/babycourageous/11ty-netlify-starter.git",
  "author": "Rene Dellefont <renedellefont@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start": "yarn run dev",
    "dev": "npm-run-all --serial build:assets build:11ty serve",
    "prebuild": "yarn run clean",
    "build": "NODE_ENV=production npm-run-all --sequential build:assets build:11ty",
    "build:assets": "npm-run-all --parallel build:assets:*",
    "build:assets:css": "postcss src/_assets/css/index.css -o _site/css/styles.css",
    "build:assets:js": "parcel build src/_assets/javascript/index.js --out-dir _site/javascript",
    "build:assets:images": "node imagemin.images.js",
    "build:assets:svg": "svg-sprite --svg-xmldecl=false --svg-doctype=false --symbol --symbol-inline=true --symbol-dest=src/_includes src/_assets/svg/*.svg",
    "build:11ty": "eleventy",
    "serve": "light-server --config .lightserverrc",
    "clean": "rm -rf _site && mkdir _site"
  },
  "dependencies": {},
  "devDependencies": {
    "@11ty/eleventy": "^0.9.0",
    "@11ty/eleventy-plugin-rss": "^1.0.7",
    "@fullhuman/postcss-purgecss": "^1.3.0",
    "autoprefixer": "^9.6.5",
    "babel-eslint": "^10.0.3",
    "chokidar": "^3.2.2",
    "chokidar-cli": "^2.0.0",
    "cssnano": "^4.1.10",
    "eslint": "^6.5.1",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^6.4.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-prettier": "^3.1.1",
    "imagemin": "^7.0.0",
    "imagemin-mozjpeg": "^8.0.0",
    "imagemin-pngquant": "^8.0.0",
    "light-server": "^2.6.4",
    "luxon": "^1.19.3",
    "npm-run-all": "^4.1.5",
    "parcel-bundler": "^1.12.4",
    "postcss-cli": "^6.1.3",
    "postcss-import": "^12.0.1",
    "precss": "^4.0.0",
    "prettier": "^1.18.2",
    "svg-sprite": "^1.5.0",
    "tailwindcss": "^1.1.2"
  }
}
